{
    "name": "TelecomsCloud",
    "apiVersion": "v1",
    "description": "TelecomsCloud REST API Client",
    "operations": {
        "formatNumberE164": {
            "httpMethod": "GET",
            "uri": "/v1/format/number/e164/{number}/{location}{?pretty}",
            "responseModel": "getJsonResponse",
            "parameters": {
                "number": {
                    "type": "string",
                    "location": "uri",
                    "required": true
                },
                "location": {
                    "type": "string",
                    "location": "uri",
                    "required": true
                },
                "pretty": {
                    "type": "string",
                    "location": "query",
                    "required": false
                }
            }
        },
        "formatNumberNational": {
            "httpMethod": "GET",
            "uri": "/v1/format/number/national/{number}/{location}{?pretty}",
            "responseModel": "getJsonResponse",
            "parameters": {
                "number": {
                    "type": "string",
                    "location": "uri",
                    "required": true
                },
                "location": {
                    "type": "string",
                    "location": "uri",
                    "required": true
                },
                "pretty": {
                    "type": "string",
                    "location": "query",
                    "required": false
                }
            }
        },
        "formatNumberInternational": {
            "httpMethod": "GET",
            "uri": "/v1/format/number/international/{number}/{providedLocation}/{dialFromLocation}{?pretty}",
            "responseModel": "getJsonResponse",
            "parameters": {
                "number": {
                    "type": "string",
                    "location": "uri",
                    "required": true
                },
                "providedLocation": {
                    "type": "string",
                    "location": "uri",
                    "required": true
                },
                "dialFromLocation": {
                    "type": "string",
                    "location": "uri",
                    "required": true
                },
                "pretty": {
                    "type": "string",
                    "location": "query",
                    "required": false
                }
            }
        },
        "formatNumberPretty": {
            "httpMethod": "GET",
            "uri": "/v1/format/number/pretty/{number}/{location}",
            "responseModel": "getJsonResponse",
            "parameters": {
                "number": {
                    "type": "string",
                    "location": "uri",
                    "required": true
                },
                "location": {
                    "type": "string",
                    "location": "uri",
                    "required": true
                }
            }
        },
        "oauth2GrantClient": {
            "httpMethod": "POST",
            "uri": "/v1/authorization/oauth2/grant-client",
            "responseModel": "getJsonResponse",
            "parameters": {
                "client_id": {
                    "type": "string",
                    "location": "json",
                    "required": true
                },
                "client_secret": {
                    "type": "string",
                    "location": "json",
                    "required": true
                }
            }
        },
        "accountInfo": {
            "httpMethod": "GET",
            "uri": "/v1/account/info",
            "responseModel": "getJsonResponse"
        },
        "getFaxInfo": {
            "httpMethod": "GET",
            "uri": "/v1/fax/inbound/info{?last_pointer}",
            "responseModel": "getJsonResponse",
            "parameters": {
                "last_pointer": {
                    "type": "string",
                    "location": "query",
                    "required": true
                }
            }
        },
        "getInboundFax": {
            "httpMethod": "GET",
            "uri": "/v1/fax/inbound/{id}",
            "responseModel": "locationLink",
            "parameters": {
                "id": {
                    "type": "string",
                    "location": "uri",
                    "required": true
                }
            }
        },
        "getSmsStatus": {
            "httpMethod": "GET",
            "uri": "/v1/sms/outbound/{smsId}/status",
            "responseModel": "getJsonResponse",
            "parameters": {
                "smsId": {
                    "type": "string",
                    "location": "uri",
                    "required": true
                }
            }
        },
        "sendSms": {
            "httpMethod": "POST",
            "uri": "/v1/sms/outbound",
            "responseModel": "getJsonResponse",
            "parameters": {
                "to": {
                    "type": "string",
                    "location": "json",
                    "required": true
                },
                "from": {
                    "type": "string",
                    "location": "json",
                    "required": true
                },
                "message": {
                    "type": "string",
                    "location": "json",
                    "required": true
                },
                "numberFormatLocation": {
                    "type": "string",
                    "location": "json",
                    "required": false
                }
            }
        }
    },
    "models": {
        "getJsonResponse": {
            "type": "object",
            "additionalProperties": {
                "location": "json"
            }
        },
        "authCodeResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "location": "header",
                    "sentAs": "location",
                    "type": "string",
                    "filters": [
                        {
                            "method": "\\Tc\\Phapic\\Filters::extractCodeFromQueryParams",
                            "args": ["@value"]
                        }
                    ]
                }
            }
        },
        "locationLink": {
            "type": "object",
            "properties": {
                "downloadLink": {
                    "location": "header",
                    "sentAs": "location",
                    "type": "string"
                }
            }
        }
    }
}